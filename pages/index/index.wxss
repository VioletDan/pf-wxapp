/**标准16:9*/
.index .bg{width: 100%;height: 151rpx;position: absolute;top: 0;}
.index .content{margin-bottom: 100rpx;}
.index .content .card{width: 750rpx;height: 414rpx;}
.index .content .inner{padding: 0 30rpx;color: #0774ff;}
.index .content .inner .date{font-size: 22rpx;margin-bottom: 45rpx;}
.index .content .inner .h4{font-size: 32rpx;margin-top: 24rpx;padding-bottom: 17rpx;position: relative;text-align: center;font-weight: bold;}
.index .content .inner .h4::after{content: '';display: block;width: 60rpx;height: 4rpx;background: #0774ff;position: absolute;bottom: 0;left: 50%;margin-left: -30rpx;border-radius: 1px;}
.index .content .inner .h5{font-size: 24rpx;text-align: center;margin-top: 30rpx;margin-bottom: 10rpx;}
.index .content .inner .txt1{font-size: 20rpx;line-height: 1.5;}
.index .content .btn{width: 750rpx;height: 169rpx;}
